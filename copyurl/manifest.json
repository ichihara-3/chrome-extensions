{
  "name": "COPY RICH URL",
  "description": "Copies url and title of the current tab as RICH TEXT, you can also choose various styles.",
  "version": "0.4.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options/index.html",
  "permissions": ["contextMenus", "storage", "activeTab", "scripting"],
  "icons": {
    "128": "img/copyurl_128.png"
  }
}
